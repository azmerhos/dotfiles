# If not running interactively, don't do anything
[[ $- != *i* ]] && return

alias ls='ls --color=auto'
alias grep='grep --color=auto'
PS1='[\u@\h \W]\$ '

export QSYS_ROOTDIR="/home/killian/intelFPGA_lite/18.1/quartus/sopc_builder/bin"
TERM=xterm-256color

fastfetch --multithreading false
